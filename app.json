{
  "expo": {
    "name": "dbookapp",
    "description": "invoice billing app",
    "slug": "dbookapp",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "7.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atsstudios.dbook"
    },
    "android": {
      "package": "com.atsstudios.dbook",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.atsstudios.dbook",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]

    }
  }
}
